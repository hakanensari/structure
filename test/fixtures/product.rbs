class Product < Data
  extend Structure::ClassMethods
  include Structure::InstanceMethods

  def self.new: (name: String?, price: Float?, tags: Array[String]?) -> instance
              | (String?, Float?, Array[String]?) -> instance

  attr_reader name: String?
  attr_reader price: Float?
  attr_reader tags: Array[String]?

  def to_h: () -> { name: String?, price: Float?, tags: Array[String]? }
end
